{
  "name": "reactjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "start:webpack": "webpack serve --open --mode development",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cypress": "cypress open",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose -f docker-compose.prod.yml build",
    "lint": "eslint src --ext .js,.ts,.tsx --fix",
    "lint-staged": "lint-staged",
    "compile": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "fix": "npx prettier -w src && npm run lint && npm run format && npx prettier -l src",
    "prepare": "cd .. && husky install frontend/.husky"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.0",
    "@mui/material": "^5.6.0",
    "@react-pdf-viewer/core": "3.8.0",
    "@reduxjs/toolkit": "^1.7.2",
    "@sentry/react": "^6.17.8",
    "@sentry/tracing": "^6.17.8",
    "@types/react-router-dom": "^5.3.3",
    "antd": "^4.19.2",
    "antd-mask-input": "^2.0.7",
    "axios": "^0.25.0",
    "decimal.js": "^10.3.1",
    "eslint-plugin-jest": "^26.1.1",
    "i18next": "^21.6.11",
    "i18next-browser-languagedetector": "^6.1.3",
    "i18next-http-backend": "^1.3.2",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "lodash.omit": "^4.5.0",
    "moment": "^2.29.2",
    "normalizr": "^3.6.1",
    "pdfjs-dist": "3.1.81",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.31.3",
    "react-i18next": "^11.15.4",
    "react-query": "^3.34.16",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.1.2",
    "react-scripts": "5.0.0",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "reselect": "^4.1.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.isempty": "^4.4.7",
    "@types/lodash.omit": "^4.5.7",
    "@types/node": "^17.0.16",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.12.0",
    "autoprefixer": "^10.4.2",
    "clean-webpack-plugin": "^4.0.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jslint-configs": "^3.0.0",
    "lint-staged": "^12.3.3",
    "postcss": "^8.4.6",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.0.22",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run compile"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn eslint --ext .ts,.tsx --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
