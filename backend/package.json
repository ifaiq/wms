{
  "name": "wms-backend-boilerplate",
  "version": "0.0.1",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prisma:migrate": "npx prisma migrate deploy",
    "prisma:dev:generate": "npx prisma generate",
    "db:dev:rm": "docker compose rm dev-db -s -f -v",
    "db:dev:up": "docker compose up dev-db -d",
    "db:dev:restart": "yarn db:dev:rm && yarn db:dev:up && sleep 1 && yarn prisma:dev:deploy",
    "prisma:test:deploy": "dotenv -e .env.test -- npx prisma migrate deploy",
    "db:test:rm": "docker compose rm test-db -s -f -v",
    "db:test:up": "docker compose up test-db -d",
    "db:test:restart": "yarn db:test:rm && yarn db:test:up && sleep 1 && yarn prisma:test:deploy",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down -v",
    "test:docker": "yarn jest src/",
    "fix": "npx prettier -w src && npm run lint && npx prisma format && npx prettier -l src",
    "prepare": "cd .. && husky install backend/.husky"
  },
  "dependencies": {
    "@development-team20/auth-library": "^1.6.0",
    "@elastic/app-search-node": "^7.16.1-beta.1",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/passport": "8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@opensearch-project/opensearch": "^1.1.0",
    "@prisma/client": "3.12",
    "@sentry/node": "^7.15.0",
    "@sentry/tracing": "^7.15.0",
    "aws-sdk": "^2.1211.0",
    "bcrypt": "5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "^0.13.2",
    "csv-parse": "^5.0.4",
    "ejs": "^3.1.6",
    "html-pdf": "^3.0.1",
    "ioredis": "^5.2.2",
    "lodash.isempty": "^4.4.0",
    "lodash.isfinite": "^3.3.2",
    "lodash.isstring": "^4.0.1",
    "lodash.isundefined": "^3.0.1",
    "lodash.sumby": "^4.6.0",
    "lodash.uniq": "^4.5.0",
    "nest-winston": "^1.6.2",
    "passport": "0.5.2",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sqs-consumer": "^5.7.0",
    "swagger-ui-express": "^4.5.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.4.0",
    "@types/bcrypt": "5.0.0",
    "@types/cache-manager": "^3.4.3",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/html-pdf": "^3.0.0",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "27.4.1",
    "@types/lodash.isempty": "^4.4.7",
    "@types/lodash.isfinite": "^3.3.7",
    "@types/lodash.isstring": "^4.0.7",
    "@types/lodash.isundefined": "^3.0.7",
    "@types/lodash.sumby": "^4.6.7",
    "@types/lodash.uniq": "^4.5.7",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.29",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "1.0.34",
    "@types/redlock": "^4.0.3",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.0.0",
    "depcheck": "^1.4.3",
    "dotenv-cli": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "prisma": "3.12",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.6.3"
  },
  "jest": {
    "testTimeout": 50000,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^csv-parse/sync": "<rootDir>/node_modules/csv-parse/dist/cjs/sync.cjs"
    }
  },
  "optionalDependencies": {
    "webpack": "^5.70.0"
  }
}
